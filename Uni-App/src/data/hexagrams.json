[
    {
        "id": 1,
        "name": "乾",
        "symbol": "111111",
        "description": "元亨利贞。",
        "use": "用九：见群龙无首，吉。",
        "lines": {
            "1": "潜龙勿用。",
            "2": "见龙在田，利见大人。",
            "3": "君子终日乾乾，夕惕若，厉，无咎。",
            "4": "或跃在渊，无咎。",
            "5": "飞龙在天，利见大人。",
            "6": "亢龙有悔。"
        }
    },
    {
        "id": 2,
        "name": "坤",
        "symbol": "000000",
        "description": "元亨，利牝马之贞。君子有攸往，先迷后得主，利。西南得朋，东北丧朋。安贞吉。",
        "use": "用六：利永贞。",
        "lines": {
            "1": "履霜，坚冰至。",
            "2": "直方大，不习，无不利。",
            "3": "含章可贞。或从王事，无成有终。",
            "4": "括囊，无咎无誉。",
            "5": "黄裳，元吉。",
            "6": "龙战于野，其血玄黄。"
        }
    },
    {
        "id": 3,
        "name": "屯",
        "symbol": "100010",
        "description": "元亨利贞。勿用有攸往，利建侯。",
        "lines": {
            "1": "磐桓，利居贞，利建侯。",
            "2": "屯如邅如，乘马班如。匪寇婚媾，女子贞不字，十年乃字。",
            "3": "即鹿无虞，惟入于林中，君子几不如舍，往吝。",
            "4": "乘马班如，求婚媾，往吉，无不利。",
            "5": "屯其膏，小贞吉，大贞凶。",
            "6": "乘马班如，泣血涟如。"
        }
    },
    {
        "id": 4,
        "name": "蒙",
        "symbol": "010001",
        "description": "亨。匪我求童蒙，童蒙求我。初筮告，再三渎，渎则不告。利贞。",
        "lines": {
            "1": "发蒙，利用刑人，用说桎梏，以往吝。",
            "2": "包蒙吉；纳妇吉；子克家。",
            "3": "勿用取女；见金夫，不有躬，无攸利。",
            "4": "困蒙，吝。",
            "5": "童蒙，吉。",
            "6": "击蒙；不利为寇，利御寇。"
        }
    },
    {
        "id": 5,
        "name": "需",
        "symbol": "111010",
        "description": "有孚，光亨，贞吉。利涉大川。",
        "lines": {
            "1": "需于郊。利用恒，无咎。",
            "2": "需于沙。小有言，终吉。",
            "3": "需于泥，致寇至。",
            "4": "需于血，出自穴。",
            "5": "需于酒食，贞吉。",
            "6": "入于穴，有不速之客三人来，敬之终吉。"
        }
    },
    {
        "id": 6,
        "name": "讼",
        "symbol": "010111",
        "description": "有孚，窒惕，中吉。终凶。利见大人，不利涉大川。",
        "lines": {
            "1": "不永所事，小有言，终吉。",
            "2": "不克讼，归而逋，其邑人三百户，无眚。",
            "3": "食旧德，贞厉，终吉，或从王事，无成。",
            "4": "不克讼，复即命，渝安贞，吉。",
            "5": "讼元吉。",
            "6": "或锡之鞶带，终朝三禠之。"
        }
    },
    {
        "id": 7,
        "name": "师",
        "symbol": "010000",
        "description": "贞，丈人，吉无咎。",
        "lines": {
            "1": "师出以律，否臧凶。",
            "2": "在师中，吉无咎，王三锡命。",
            "3": "师或舆尸，凶。",
            "4": "师左次，无咎。",
            "5": "田有禽，利执言，无咎。长子帅师，弟子舆尸，贞凶。",
            "6": "大君有命，开国承家，小人勿用。"
        }
    },
    {
        "id": 8,
        "name": "比",
        "symbol": "000010",
        "description": "吉。原筮元永贞，无咎。不宁方来，后夫凶。",
        "lines": {
            "1": "有孚比之，无咎。有孚盈缶，终来有他吉。",
            "2": "比之自内，贞吉。",
            "3": "比之匪人。",
            "4": "外比之，贞吉。",
            "5": "显比，王用三驱，失前禽。邑人不诫，吉。",
            "6": "比之无首，凶。"
        }
    },
    {
        "id": 9,
        "name": "小畜",
        "symbol": "111011",
        "description": "亨。密云不雨，自我西郊。",
        "lines": {
            "1": "复自道，何其咎？吉。",
            "2": "牵复，吉。",
            "3": "舆说辐，夫妻反目。",
            "4": "有孚，血去惕出，无咎。",
            "5": "有孚挛如，富以其邻。",
            "6": "既雨既处，尚德载。妇贞厉。月几望，君子征凶。"
        }
    },
    {
        "id": 10,
        "name": "履",
        "symbol": "110111",
        "description": "履虎尾，不咥人，亨。",
        "lines": {
            "1": "素履，往无咎。",
            "2": "履道坦坦，幽人贞吉。",
            "3": "眇能视，跛能履，履虎尾，咥人，凶。武人为于大君。",
            "4": "履虎尾，愬愬，终吉。",
            "5": "夬履，贞厉。",
            "6": "视履考祥，其旋元吉。"
        }
    },
    {
        "id": 11,
        "name": "泰",
        "symbol": "111000",
        "description": "小往大来，吉亨。",
        "lines": {
            "1": "拔茅茹，以其汇，征吉。",
            "2": "包荒，用冯河，不遐遗，朋亡，得尚于中行。",
            "3": "无平不陂，无往不复，艰贞无咎。勿恤其孚，于食有福。",
            "4": "翩翩，不富以其邻，不戒以孚。",
            "5": "帝乙归妹，以祉元吉。",
            "6": "城复于隍，勿用师。自邑告命，贞吝。"
        }
    },
    {
        "id": 12,
        "name": "否",
        "symbol": "000111",
        "description": "否之匪人，不利君子贞，大往小来。",
        "lines": {
            "1": "拔茅茹，以其汇，贞吉亨。",
            "2": "包承，小人吉，大人否亨。",
            "3": "包羞。",
            "4": "有命无咎，畴离祉。",
            "5": "休否，大人吉。其亡其亡，系于苞桑。",
            "6": "倾否，先否后喜。"
        }
    },
    {
        "id": 13,
        "name": "同人",
        "symbol": "101111",
        "description": "同人于野，亨。利涉大川，利君子贞。",
        "lines": {
            "1": "同人于门，无咎。",
            "2": "同人于宗，吝。",
            "3": "伏戎于莽，升其高陵，三岁不兴。",
            "4": "乘其墉，弗克攻，吉。",
            "5": "同人，先号咷而后笑。大师克相遇。",
            "6": "同人于郊，无悔。"
        }
    },
    {
        "id": 14,
        "name": "大有",
        "symbol": "111101",
        "description": "元亨。",
        "lines": {
            "1": "无交害，匪咎，艰则无咎。",
            "2": "大车以载，有攸往，无咎。",
            "3": "公用亨于天子，小人弗克。",
            "4": "匪其彭，无咎。",
            "5": "厥孚交如，威如，吉。",
            "6": "自天佑之，吉无不利。"
        }
    },
    {
        "id": 15,
        "name": "谦",
        "symbol": "001000",
        "description": "亨，君子有终。",
        "lines": {
            "1": "谦谦君子，用涉大川，吉。",
            "2": "鸣谦，贞吉。",
            "3": "劳谦，君子有终，吉。",
            "4": "无不利，撝谦。",
            "5": "不富以其邻，利用侵伐，无不利。",
            "6": "鸣谦，利用行师，征邑国。"
        }
    },
    {
        "id": 16,
        "name": "豫",
        "symbol": "000100",
        "description": "利建侯行师。",
        "lines": {
            "1": "鸣豫，凶。",
            "2": "介于石，不终日，贞吉。",
            "3": "盱豫，悔。迟有悔。",
            "4": "由豫，大有得。勿疑。朋盍簪。",
            "5": "贞疾，恒不死。",
            "6": "冥豫，成有渝，无咎。"
        }
    },
    {
        "id": 17,
        "name": "随",
        "symbol": "100110",
        "description": "元亨利贞，无咎。",
        "lines": {
            "1": "官有渝，贞吉。出门交有功。",
            "2": "系小子，失丈夫。",
            "3": "系丈夫，失小子。随有求得，利居贞。",
            "4": "随有获，贞凶。有孚在道，以明，何咎。",
            "5": "孚于嘉，吉。",
            "6": "拘系之，乃从维之。王用亨于西山。"
        }
    },
    {
        "id": 18,
        "name": "蛊",
        "symbol": "011001",
        "description": "元亨，利涉大川。先甲三日，后甲三日。",
        "lines": {
            "1": "干父之蛊，有子，考无咎，厉终吉。",
            "2": "干母之蛊，不可贞。",
            "3": "干父之蛊，小有悔，无大咎。",
            "4": "裕父之蛊，往见吝。",
            "5": "干父之蛊，用誉。",
            "6": "不事王侯，高尚其事。"
        }
    },
    {
        "id": 19,
        "name": "临",
        "symbol": "110000",
        "description": "元亨利贞。至于八月有凶。",
        "lines": {
            "1": "咸临，贞吉。",
            "2": "咸临，吉无不利。",
            "3": "甘临，无攸利。既忧之，无咎。",
            "4": "至临，无咎。",
            "5": "知临，大君之宜，吉。",
            "6": "敦临，吉无咎。"
        }
    },
    {
        "id": 20,
        "name": "观",
        "symbol": "000011",
        "description": "盥而不荐，有孚颙若。",
        "lines": {
            "1": "童观，小人无咎，君子吝。",
            "2": "窥观，利女贞。",
            "3": "观我生，进退。",
            "4": "观国之光，利用宾于王。",
            "5": "观我生，君子无咎。",
            "6": "观其生，君子无咎。"
        }
    },
    {
        "id": 21,
        "name": "噬嗑",
        "symbol": "100101",
        "description": "亨。利用狱。",
        "lines": {
            "1": "屦校灭趾，无咎。",
            "2": "噬肤灭鼻，无咎。",
            "3": "噬腊肉遇毒，小吝，无咎。",
            "4": "噬干胏，得金矢，利艰贞，吉。",
            "5": "噬干肉得黄金，贞厉，无咎。",
            "6": "何校灭耳，凶。"
        }
    },
    {
        "id": 22,
        "name": "贲",
        "symbol": "101001",
        "description": "亨。小利有攸往。",
        "lines": {
            "1": "贲其趾，舍车而徒。",
            "2": "贲其须。",
            "3": "贲如濡如，永贞吉。",
            "4": "贲如皤如，白马翰如，匪寇婚媾。",
            "5": "贲于丘园，束帛戋戋，吝，终吉。",
            "6": "白贲，无咎。"
        }
    },
    {
        "id": 23,
        "name": "剥",
        "symbol": "000001",
        "description": "不利有攸往。",
        "lines": {
            "1": "剥床以足，蔑贞凶。",
            "2": "剥床以辨，蔑贞凶。",
            "3": "剥之，无咎。",
            "4": "剥床以肤，凶。",
            "5": "贯鱼，以宫人宠，无不利。",
            "6": "硕果不食，君子得舆，小人剥庐。"
        }
    },
    {
        "id": 24,
        "name": "复",
        "symbol": "100000",
        "description": "亨。出入无疾，朋来无咎。反复其道，七日来复。利有攸往。",
        "lines": {
            "1": "不远复，无祗悔，元吉。",
            "2": "休复，吉。",
            "3": "频复，厉无咎。",
            "4": "中行独复。",
            "5": "敦复，无悔。",
            "6": "迷复，凶，有灾眚。用行师，终有大败，以其国君凶，至于十年不克征。"
        }
    },
    {
        "id": 25,
        "name": "无妄",
        "symbol": "100111",
        "description": "元亨，利贞。其匪正有眚，不利有攸往。",
        "lines": {
            "1": "无妄，往吉。",
            "2": "不耕获，不菑畲，则利有攸往。",
            "3": "无妄之灾，或系之牛，行人之得，邑人之灾。",
            "4": "可贞，无咎。",
            "5": "无妄之疾，勿药有喜。",
            "6": "无妄，行有眚，无攸利。"
        }
    },
    {
        "id": 26,
        "name": "大畜",
        "symbol": "111001",
        "description": "利贞。不家食吉，利涉大川。",
        "lines": {
            "1": "有厉，利已。",
            "2": "舆说輹。",
            "3": "良马逐，利艰贞。曰闲舆卫，利有攸往。",
            "4": "童牛之牿，元吉。",
            "5": "豶豕之牙，吉。",
            "6": "何天之衢，亨。"
        }
    },
    {
        "id": 27,
        "name": "颐",
        "symbol": "100001",
        "description": "贞吉。观颐，自求口实。",
        "lines": {
            "1": "舍尔灵龟，观我朵颐，凶。",
            "2": "颠颐，拂经，于丘颐，征凶。",
            "3": "拂颐，贞凶，十年勿用，无攸利。",
            "4": "颠颐，吉。虎视眈眈，其欲逐逐，无咎。",
            "5": "拂经，居贞吉，不可涉大川。",
            "6": "由颐，厉吉，利涉大川。"
        }
    },
    {
        "id": 28,
        "name": "大过",
        "symbol": "011110",
        "description": "栋桡，利有攸往，亨。",
        "lines": {
            "1": "藉用白茅，无咎。",
            "2": "枯杨生稊，老夫得其女妻，无不利。",
            "3": "栋桡，凶。",
            "4": "栋隆，吉。有它吝。",
            "5": "枯杨生华，老妇得其士夫，无咎无誉。",
            "6": "过涉灭顶，凶，无咎。"
        }
    },
    {
        "id": 29,
        "name": "坎",
        "symbol": "010010",
        "description": "习坎，有孚，维心亨，行有尚。",
        "lines": {
            "1": "习坎，入于坎窞，凶。",
            "2": "坎有险，求小得。",
            "3": "来之坎坎，险且枕，入于坎窞，勿用。",
            "4": "樽酒簋贰，用缶，纳约自牖，终无咎。",
            "5": "坎不盈，祗既平，无咎。",
            "6": "系用徽纆，置于丛棘，三岁不得，凶。"
        }
    },
    {
        "id": 30,
        "name": "离",
        "symbol": "101101",
        "description": "利贞，亨。畜牝牛，吉。",
        "lines": {
            "1": "履错然，敬之无咎。",
            "2": "黄离，元吉。",
            "3": "日昃之离，不鼓缶而歌，则大耋之嗟，凶。",
            "4": "突如其来如，焚如，死如，弃如。",
            "5": "出涕沱若，戚嗟若，吉。",
            "6": "王用出征，有嘉折首，获匪其丑，无咎。"
        }
    },
    {
        "id": 31,
        "name": "咸",
        "symbol": "001110",
        "description": "亨，利贞。取女吉。",
        "lines": {
            "1": "咸其拇。",
            "2": "咸其腓，凶，居吉。",
            "3": "咸其股，执其随，往吝。",
            "4": "贞吉悔亡，憧憧往来，朋从尔思。",
            "5": "咸其脢，无悔。",
            "6": "咸其辅颊舌。"
        }
    },
    {
        "id": 32,
        "name": "恒",
        "symbol": "011100",
        "description": "亨，无咎，利贞，利有攸往。",
        "lines": {
            "1": "浚恒，贞凶，无攸利。",
            "2": "悔亡。",
            "3": "不恒其德，或承之羞，贞吝。",
            "4": "田无禽。",
            "5": "恒其德，贞，妇人吉，夫子凶。",
            "6": "振恒，凶。"
        }
    },
    {
        "id": 33,
        "name": "遁",
        "symbol": "001111",
        "description": "亨。小利贞。",
        "lines": {
            "1": "遁尾，厉，勿用有攸往。",
            "2": "执之用黄牛之革，莫之胜说。",
            "3": "系遁，有疾厉，畜臣妾吉。",
            "4": "好遁，君子吉，小人否。",
            "5": "嘉遁，贞吉。",
            "6": "肥遁，无不利。"
        }
    },
    {
        "id": 34,
        "name": "大壮",
        "symbol": "111100",
        "description": "利贞。",
        "lines": {
            "1": "壮于趾，征凶，有孚。",
            "2": "贞吉。",
            "3": "小人用壮，君子用罔，贞厉。羝羊触藩，羸其角。",
            "4": "贞吉悔亡，藩决不羸，壮于大舆之輹。",
            "5": "丧羊于易，无悔。",
            "6": "羝羊触藩，不能退，不能遂，无攸利，艰则吉。"
        }
    },
    {
        "id": 35,
        "name": "晋",
        "symbol": "000101",
        "description": "康侯用锡马蕃庶，昼日三接。",
        "lines": {
            "1": "晋如摧如，贞吉。罔孚，裕无咎。",
            "2": "晋如愁如，贞吉。受兹介福，于其王母。",
            "3": "众允，悔亡。",
            "4": "晋如鼫鼠，贞厉。",
            "5": "悔亡，失得勿恤，往吉，无不利。",
            "6": "晋其角，维用伐邑，厉吉无咎，贞吝。"
        }
    },
    {
        "id": 36,
        "name": "明夷",
        "symbol": "101000",
        "description": "利艰贞。",
        "lines": {
            "1": "明夷于飞，垂其翼。君子于行，三日不食。有攸往，主人有言。",
            "2": "明夷，夷于左股，用拯马壮，吉。",
            "3": "明夷于南狩，得其大首，不可疾贞。",
            "4": "入于左腹，获明夷之心，出于门庭。",
            "5": "箕子之明夷，利贞。",
            "6": "不明晦，初登于天，后入于地。"
        }
    },
    {
        "id": 37,
        "name": "家人",
        "symbol": "101011",
        "description": "利女贞。",
        "lines": {
            "1": "闲有家，悔亡。",
            "2": "无攸遂，在中馈，贞吉。",
            "3": "家人嗃嗃，悔厉吉；妇子嘻嘻，终吝。",
            "4": "富家，大吉。",
            "5": "王假有家，勿恤，吉。",
            "6": "有孚威如，终吉。"
        }
    },
    {
        "id": 38,
        "name": "睽",
        "symbol": "110101",
        "description": "小事吉。",
        "lines": {
            "1": "悔亡，丧马勿逐，自复。见恶人，无咎。",
            "2": "遇主于巷，无咎。",
            "3": "见舆曳，其牛掣，其人天且劓，无初有终。",
            "4": "睽孤，遇元夫，交孚，厉无咎。",
            "5": "悔亡，厥宗噬肤，往何咎。",
            "6": "睽孤，见豕负涂，载鬼一车，先张之弧，后说之弧。匪寇婚媾，往遇雨则吉。"
        }
    },
    {
        "id": 39,
        "name": "蹇",
        "symbol": "001010",
        "description": "利西南，不利东北。利见大人，贞吉。",
        "lines": {
            "1": "往蹇，来誉。",
            "2": "王臣蹇蹇，匪躬之故。",
            "3": "往蹇，来反。",
            "4": "往蹇，来连。",
            "5": "大蹇，朋来。",
            "6": "往蹇，来硕，吉；利见大人。"
        }
    },
    {
        "id": 40,
        "name": "解",
        "symbol": "010100",
        "description": "利西南。无所往，其来复吉。有攸往，夙吉。",
        "lines": {
            "1": "无咎。",
            "2": "田获三狐，得黄矢，贞吉。",
            "3": "负且乘，致寇至，贞吝。",
            "4": "解而拇，朋至斯孚。",
            "5": "君子维有解，吉，有孚于小人。",
            "6": "公用射隼于高墉之上，获之，无不利。"
        }
    },
    {
        "id": 41,
        "name": "损",
        "symbol": "110001",
        "description": "有孚，元吉，无咎，可贞，利有攸往。曷之用？二簋可用享。",
        "lines": {
            "1": "已事遄往，无咎。酌损之。",
            "2": "利贞，征凶。弗损益之。",
            "3": "三人行，则损一人；一人行，则得其友。",
            "4": "损其疾，使遄有喜，无咎。",
            "5": "或益之十朋之龟，弗克违，元吉。",
            "6": "弗损益之，无咎，贞吉，利有攸往，得臣无家。"
        }
    },
    {
        "id": 42,
        "name": "益",
        "symbol": "100011",
        "description": "利有攸往，利涉大川。",
        "lines": {
            "1": "利用为大作，元吉，无咎。",
            "2": "或益之十朋之龟，弗克违，永贞吉。王用享于帝，吉。",
            "3": "益之用凶事，无咎。有孚中行，告公用圭。",
            "4": "中行，告公从。利用为依迁国。",
            "5": "有孚惠心，勿问元吉。有孚惠我德。",
            "6": "莫益之，或击之，立心勿恒，凶。"
        }
    },
    {
        "id": 43,
        "name": "夬",
        "symbol": "111110",
        "description": "扬于王庭，孚号，有厉。告自邑，不利即戎，利有攸往。",
        "lines": {
            "1": "壮于前趾，往不胜为咎。",
            "2": "惕号，莫夜有戎，勿恤。",
            "3": "壮于頄，有凶。君子夬夬，独行遇雨，若濡，有愠，无咎。",
            "4": "臀无肤，其行次且。牵羊悔亡，闻言不信。",
            "5": "苋陆夬夬，中行无咎。",
            "6": "无号，终有凶。"
        }
    },
    {
        "id": 44,
        "name": "姤",
        "symbol": "011111",
        "description": "女壮，勿用取女。",
        "lines": {
            "1": "系于金柅，贞吉。有攸往，见凶，羸豕孚蹢躅。",
            "2": "包有鱼，无咎，不利宾。",
            "3": "臀无肤，其行次且，厉，无大咎。",
            "4": "包无鱼，起凶。",
            "5": "以杞包瓜，含章，有陨自天。",
            "6": "姤其角，吝，无咎。"
        }
    },
    {
        "id": 45,
        "name": "萃",
        "symbol": "000110",
        "description": "亨。王假有庙。利见大人，亨，利贞。用大牲吉，利有攸往。",
        "lines": {
            "1": "有孚不终，乃乱乃萃。若号一握为笑，勿恤，往无咎。",
            "2": "引吉，无咎。孚乃利用禴。",
            "3": "萃如嗟如，无攸利。往无咎，小吝。",
            "4": "大吉，无咎。",
            "5": "萃有位，无咎。匪孚，元永贞，悔亡。",
            "6": "赍咨涕洟，无咎。"
        }
    },
    {
        "id": 46,
        "name": "升",
        "symbol": "011000",
        "description": "元亨。用见大人，勿恤。南征吉。",
        "lines": {
            "1": "允升，大吉。",
            "2": "孚乃利用禴，无咎。",
            "3": "升虚邑。",
            "4": "王用亨于岐山，吉，无咎。",
            "5": "贞吉，升阶。",
            "6": "冥升，利于不息之贞。"
        }
    },
    {
        "id": 47,
        "name": "困",
        "symbol": "010110",
        "description": "亨。贞大人吉，无咎。有言不信。",
        "lines": {
            "1": "臀困于株木，入于幽谷，三岁不觌。",
            "2": "困于酒食，朱绂方来，利用享祀，征凶，无咎。",
            "3": "困于石，据于蒺藜，入于其宫，不见其妻，凶。",
            "4": "来徐徐，困于金车，吝，有终。",
            "5": "劓刖，困于赤绂，乃徐有说，利用祭祀。",
            "6": "困于葛藟，于臲卼，曰动悔。有悔，征吉。"
        }
    },
    {
        "id": 48,
        "name": "井",
        "symbol": "011010",
        "description": "改邑不改井，无丧无得。往来井井。汔至，亦未繘井，羸其瓶，凶。",
        "lines": {
            "1": "井泥不食，旧井无禽。",
            "2": "井谷射鲋，瓮敝漏。",
            "3": "井渫不食，为我心恻，可用汲，王明并受其福。",
            "4": "井甃，无咎。",
            "5": "井洌，寒泉食。",
            "6": "井收勿幕，有孚元吉。"
        }
    },
    {
        "id": 49,
        "name": "革",
        "symbol": "101110",
        "description": "己日乃孚。元亨利贞，悔亡。",
        "lines": {
            "1": "巩用黄牛之革。",
            "2": "己日乃革之，征吉，无咎。",
            "3": "征凶，贞厉。革言三就，有孚。",
            "4": "悔亡，有孚改命，吉。",
            "5": "大人虎变，未占有孚。",
            "6": "君子豹变，小人革面，征凶，居贞吉。"
        }
    },
    {
        "id": 50,
        "name": "鼎",
        "symbol": "011101",
        "description": "元吉，亨。",
        "lines": {
            "1": "鼎颠趾，利出否。得妾以其子，无咎。",
            "2": "鼎有实，我仇有疾，不我能即，吉。",
            "3": "鼎耳革，其行塞，雉膏不食，方雨亏悔，终吉。",
            "4": "鼎折足，覆公餗，其形渥，凶。",
            "5": "鼎黄耳金铉，利贞。",
            "6": "鼎玉铉，大吉，无不利。"
        }
    },
    {
        "id": 51,
        "name": "震",
        "symbol": "100100",
        "description": "亨。震来虩虩，笑言哑哑。震惊百里，不丧匕鬯。",
        "lines": {
            "1": "震来虩虩，后笑言哑哑，吉。",
            "2": "震来厉，亿丧贝，跻于九陵，勿逐，七日得。",
            "3": "震苏苏，震行无眚。",
            "4": "震遂泥。",
            "5": "震往来厉，亿无丧，有事。",
            "6": "震索索，视矍矍，征凶。震不于其躬，于其邻，无咎。婚媾有言。"
        }
    },
    {
        "id": 52,
        "name": "艮",
        "symbol": "001001",
        "description": "艮其背，不获其身，行其庭，不见其人，无咎。",
        "lines": {
            "1": "艮其趾，无咎。利永贞。",
            "2": "艮其腓，不拯其随，其心不快。",
            "3": "艮其限，列其夤，厉薰心。",
            "4": "艮其身，无咎。",
            "5": "艮其辅，言有序，悔亡。",
            "6": "敦艮，吉。"
        }
    },
    {
        "id": 53,
        "name": "渐",
        "symbol": "001011",
        "description": "女归吉，利贞。",
        "lines": {
            "1": "鸿渐于干，小子厉，有言，无咎。",
            "2": "鸿渐于磐，饮食衎衎，吉。",
            "3": "鸿渐于陆，夫征不复，妇孕不育，凶；利御寇。",
            "4": "鸿渐于木，或得其桷，无咎。",
            "5": "鸿渐于陵，妇三岁不孕，终莫之胜，吉。",
            "6": "鸿渐于陆，其羽可用为仪，吉。"
        }
    },
    {
        "id": 54,
        "name": "归妹",
        "symbol": "110100",
        "description": "征凶，无攸利。",
        "lines": {
            "1": "归妹以娣，跛能履，征吉。",
            "2": "眇能视，利幽人之贞。",
            "3": "归妹以须，反归以娣。",
            "4": "归妹愆期，迟归有时。",
            "5": "帝乙归妹，其君之袂，不如其娣之袂良。月几望，吉。",
            "6": "女承筐无实，士刲羊无血，无攸利。"
        }
    },
    {
        "id": 55,
        "name": "丰",
        "symbol": "101100",
        "description": "亨。王假之，勿忧，宜日中。",
        "lines": {
            "1": "遇其配主，虽旬无咎，往有尚。",
            "2": "丰其蔀，日中见斗。往得疑疾，有孚发若，吉。",
            "3": "丰其沛，日中见沬。折其右肱，无咎。",
            "4": "丰其蔀，日中见斗。遇其夷主，吉。",
            "5": "来章，有庆誉，吉。",
            "6": "丰其屋，蔀其家，窥其户，阒其无人，三岁不觌，凶。"
        }
    },
    {
        "id": 56,
        "name": "旅",
        "symbol": "001101",
        "description": "小亨，旅贞吉。",
        "lines": {
            "1": "旅琐琐，斯其所取灾。",
            "2": "旅即次，怀其资，得童仆，贞。",
            "3": "旅焚其次，丧其童仆，贞厉。",
            "4": "旅于处，得其资斧，我心不快。",
            "5": "射雉，一矢亡，终以誉命。",
            "6": "鸟焚其巢，旅人先笑后号咷。丧牛于易，凶。"
        }
    },
    {
        "id": 57,
        "name": "巽",
        "symbol": "011011",
        "description": "小亨，利有攸往，利见大人。",
        "lines": {
            "1": "进退，利武人之贞。",
            "2": "巽在床下，用史巫纷若，吉，无咎。",
            "3": "频巽，吝。",
            "4": "悔亡，田获三品。",
            "5": "贞吉，悔亡，无不利。无初有终。先庚三日，后庚三日，吉。",
            "6": "巽在床下，丧其资斧，贞凶。"
        }
    },
    {
        "id": 58,
        "name": "兑",
        "symbol": "110110",
        "description": "亨，利贞。",
        "lines": {
            "1": "和兑，吉。",
            "2": "孚兑，吉，悔亡。",
            "3": "来兑，凶。",
            "4": "商兑，未宁，介疾有喜。",
            "5": "孚于剥，有厉。",
            "6": "引兑。"
        }
    },
    {
        "id": 59,
        "name": "涣",
        "symbol": "010011",
        "description": "亨。王假有庙，利涉大川，利贞。",
        "lines": {
            "1": "用拯马壮，吉。",
            "2": "涣奔其机，悔亡。",
            "3": "涣其躬，无悔。",
            "4": "涣其群，元吉。涣有丘，匪夷所思。",
            "5": "涣汗其大号，涣王居，无咎。",
            "6": "涣其血，去逖出，无咎。"
        }
    },
    {
        "id": 60,
        "name": "节",
        "symbol": "110010",
        "description": "亨。苦节不可贞。",
        "lines": {
            "1": "不出户庭，无咎。",
            "2": "不出门庭，凶。",
            "3": "不节若，则嗟若，无咎。",
            "4": "安节，亨。",
            "5": "甘节，吉；往有尚。",
            "6": "苦节，贞凶，悔亡。"
        }
    },
    {
        "id": 61,
        "name": "中孚",
        "symbol": "110011",
        "description": "豚鱼，吉。利涉大川，利贞。",
        "lines": {
            "1": "虞吉，有它不燕。",
            "2": "鸣鹤在阴，其子和之。我有好爵，吾与尔靡之。",
            "3": "得敌，或鼓或罢，或泣或歌。",
            "4": "月几望，马匹亡，无咎。",
            "5": "有孚挛如，无咎。",
            "6": "翰音登于天，贞凶。"
        }
    },
    {
        "id": 62,
        "name": "小过",
        "symbol": "001100",
        "description": "亨，利贞。可小事，不可大事。飞鸟遗之音，不宜上，宜下，大吉。",
        "lines": {
            "1": "飞鸟以凶。",
            "2": "过其祖，遇其妣。不及其君，遇其臣，无咎。",
            "3": "弗过防之，从或戕之，凶。",
            "4": "无咎。弗过遇之。往厉必戒，勿用永贞。",
            "5": "密云不雨，自我西郊。公弋取彼在穴。",
            "6": "弗遇过之，飞鸟离之，凶，是谓灾眚。"
        }
    },
    {
        "id": 63,
        "name": "既济",
        "symbol": "101010",
        "description": "亨，小利贞，初吉终乱。",
        "lines": {
            "1": "曳其轮，濡其尾，无咎。",
            "2": "妇丧其茀，勿逐，七日得。",
            "3": "高宗伐鬼方，三年克之，小人勿用。",
            "4": "繻有衣袽，终日戒。",
            "5": "东邻杀牛，不如西邻之禴祭，实受其福。",
            "6": "濡其首，厉。"
        }
    },
    {
        "id": 64,
        "name": "未济",
        "symbol": "010101",
        "description": "亨，小狐汔济，濡其尾，无攸利。",
        "lines": {
            "1": "濡其尾，吝。",
            "2": "曳其轮，贞吉。",
            "3": "未济，征凶，利涉大川。",
            "4": "贞吉，悔亡，震用伐鬼方，三年有赏于大国。",
            "5": "贞吉，无悔。君子之光，有孚，吉。",
            "6": "有孚于饮酒，无咎。濡其首，有孚失是。"
        }
    }
]
