[
    {
        "id": 1,
        "name": "乾",
        "symbol": "111111",
        "description": "元，亨，利，贞。",
        "interpretation": "大哉乾元，万物资始，乃统天。",
        "lines": {
            "1": "潜龙勿用。",
            "2": "见龙在田，利见大人。",
            "3": "君子终日乾乾，夕惕若，厉无咎。",
            "4": "或跃在渊，无咎。",
            "5": "飞龙在天，利见大人。",
            "6": "亢龙有悔。"
        }
    },
    {
        "id": 2,
        "name": "坤",
        "symbol": "000000",
        "description": "元，亨，利牝马之贞。",
        "interpretation": "至哉坤元，万物资生，乃顺承天。",
        "lines": {
            "1": "履霜，坚冰至。",
            "2": "直，方，大，不习无不利。",
            "3": "含章可贞。或从王事，无成有终。",
            "4": "括囊，无咎，无誉。",
            "5": "黄裳，元吉。",
            "6": "龙战于野，其血玄黄。"
        }
    },
    {
        "id": 3,
        "name": "屯",
        "symbol": "100010",
        "description": "元，亨，利，贞。勿用有攸往，利建侯。",
        "interpretation": "屯，刚柔始交而难生，动在险中。",
        "lines": {
            "1": "磐桓，利居贞，利建侯。",
            "2": "屯如邅如，乘马班如。匪寇婚媾，女子贞不字，十年乃字。",
            "3": "即鹿无虞，惟入于林中，君子几不如舍，往吝。",
            "4": "乘马班如，求婚媾，往吉，无不利。",
            "5": "屯其膏，小贞吉，大贞凶。",
            "6": "乘马班如，泣血涟如。"
        }
    },
    {
        "id": 4,
        "name": "蒙",
        "symbol": "010001",
        "description": "亨。匪我求童蒙，童蒙求我。初噬告，再三渎，渎则不告。利贞。",
        "interpretation": "蒙，山下有险，险而止，蒙。",
        "lines": {
            "1": "发蒙，利用刑人，用说桎梏，以往吝。",
            "2": "包蒙吉；纳妇吉；子克家。",
            "3": "勿用取女；见金夫，不有躬，无攸利。",
            "4": "困蒙，吝。",
            "5": "童蒙，吉。",
            "6": "击蒙；不利为寇，利御寇。"
        }
    },
    {
        "id": 5,
        "name": "需",
        "symbol": "111010",
        "description": "有孚，光亨，贞吉。利涉大川。",
        "interpretation": "需，须也；险在前也。刚健而不陷，其义不困穷矣。",
        "lines": {
            "1": "需于郊。利用恒，无咎。",
            "2": "需于沙。小有言，终吉。",
            "3": "需于泥，致寇至。",
            "4": "需于血，出自穴。",
            "5": "需于酒食，贞吉。",
            "6": "入于穴，有不速之客三人来，敬之终吉。"
        }
    },
    {
        "id": 6,
        "name": "讼",
        "symbol": "010111",
        "description": "有孚，窒惕，中吉。终凶。利见大人，不利涉大川。",
        "interpretation": "讼，上刚下险，险而健，讼。",
        "lines": {
            "1": "不永所事，小有言，终吉。",
            "2": "不克讼，归而逋，其邑人三百户，无眚。",
            "3": "食旧德，贞厉，终吉，或从王事，无成。",
            "4": "不克讼，复即命，渝安贞，吉。",
            "5": "讼元吉。",
            "6": "或锡之鞶带，终朝三禠之。"
        }
    },
    {
        "id": 7,
        "name": "师",
        "symbol": "010000",
        "description": "贞，丈人，吉无咎。",
        "interpretation": "师，众也，贞正也，能以众正，可以王矣。",
        "lines": {
            "1": "师出以律，否臧凶。",
            "2": "在师中，吉无咎，王三锡命。",
            "3": "师或舆尸，凶。",
            "4": "师左次，无咎。",
            "5": "田有禽，利执言，无咎。长子帅师，弟子舆尸，贞凶。",
            "6": "大君有命，开国承家，小人勿用。"
        }
    },
    {
        "id": 8,
        "name": "比",
        "symbol": "000010",
        "description": "吉。原筮元永贞，无咎。不宁方来，后夫凶。",
        "interpretation": "比，吉也，比，辅也，下顺从也。",
        "lines": {
            "1": "有孚比之，无咎。有孚盈缶，终来有他吉。",
            "2": "比之自内，贞吉。",
            "3": "比之匪人。",
            "4": "外比之，贞吉。",
            "5": "显比，王用三驱，失前禽。邑人不诫，吉。",
            "6": "比之无首，凶。"
        }
    }
]